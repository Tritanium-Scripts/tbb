<!-- FAQRankTable -->
<table cellpadding="3" cellspacing="0">
 <tr><th>{Language::getInstance()->getString('user_rank')}</th><th>{Language::getInstance()->getString('min_posts')}</th><th>{Language::getInstance()->getString('max_posts')}</th><th>{Language::getInstance()->getString('stars')}</th></tr>
{foreach $ranks as $curRank} <tr><td class="cellStd"><span class="fontNorm">{$curRank[1]}</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm">{$curRank[2]}</span></td><td class="cellStd" style="text-align:center;"><span class="fontNorm">{if $curRank@last}&infin;{else}{$curRank[3]}{/if}</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm">{$curRank[4]}</span></td></tr>{/foreach}
 <tr><td class="cellStd"><span class="fontNorm">{Language::getInstance()->getString('moderator')}</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellStd" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm" style="color:{Config::getInstance()->getCfgVal('wio_color_mod')}">{Config::getInstance()->getCfgVal('stars_mod')}</span></td></tr>
 <tr><td class="cellStd"><span class="fontNorm">{Language::getInstance()->getString('super_moderator')}</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellStd" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm" style="color:{Config::getInstance()->getCfgVal('wio_color_smod')}">{Config::getInstance()->getCfgVal('stars_smod')}</span></td></tr>
 <tr><td class="cellStd"><span class="fontNorm">{Language::getInstance()->getString('administrator')}</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellStd" style="text-align:center;"><span class="fontNorm">-</span></td><td class="cellAlt" style="text-align:center;"><span class="fontNorm" style="color:{Config::getInstance()->getCfgVal('wio_color_admin')}">{Config::getInstance()->getCfgVal('stars_admin')}</span></td></tr>
</table>